<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gnuoynawh.samples.sampleutilsapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleUtilsApp">
        <activity
            android:name=".base.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.foldable.FoldableActivity"/>
        <activity android:name=".ui.setting.LanguageActivity"/>
        <activity android:name=".ui.CalendarActivity"/>
        <activity android:name=".ui.HtmlParseActivity"/>
        <activity android:name=".ui.VersionCheckActivity"/>
        <activity android:name=".ui.OpenChromeActivity"/>
    </application>

</manifest>